{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "Node16", // Change this line
    "baseUrl": "src",
    "outDir": "dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly": true,
    "declaration": true
  },
  "include": ["src/**/*"]
}
